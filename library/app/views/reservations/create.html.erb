<%= form_for(@reservation) do |f| %>
<%= render 'shared/validation_errors', object: @reservation %>
<%= f.label :Title %> <%= f.text_field :title, autofocus: true %><br />
<p>
  <%= f.label :Author %>
  <%= f.collection_select :author_id, Author.all, :id , :name  %>
</p>

<P>
<%= f.label :reserved_On %>
 <span class="date">
      <%= f.date_select :reserved_on , :order => [:month, :day, :year] %>
</span>
</P>
<%= f.label :due_On %>
 <span class="date">
      <%= f.date_select :due_On , :order => [:month, :day, :year] %>
</span>
<p>
	<%= f.submit %>
	<%= button_to 'Cancel', reservations_path %>
</P>
<% end %>
